         <children>
            <ToolBar depthTest="DISABLE" style="-fx-background-color: D6D6D6;">
              <items>
                <Button id="btn_safehouse" mnemonicParsing="false" onAction="#home" style="-fx-background-color: D6D6D6;" text="SAFEHOUSE">
